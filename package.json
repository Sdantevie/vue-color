{
  "name": "@ckpack/vue-color",
  "version": "1.0.1",
  "description": "(vue3.0) ðŸŽ¨ Vue Color Pickers for Sketch, Photoshop, Chrome & more",
  "repository": "https://github.com/ckpack/vue-color",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "libs/index.js",
  "unpkg": "dist/index.min.js",
  "author": "chenkai",
  "keywords": [
    "vue-color",
    "color",
    "vuejs"
  ],
  "files": [
    "dist/*",
    "src/*",
    "libs/*"
  ],
  "scripts": {
    "dev": "yarn build && vue-cli-service serve dev/serve.js",
    "build": "yarn gen:compoents && yarn build:libs && yarn build:dist",
    "build:libs": "rm -rf ./libs && rollup --config ./build/build.libs.js",
    "build:dist": "rm -rf ./dist && rollup --config ./build/build.dist.js",
    "build:style": "",
    "build:website": "vue-cli-service build --dest website dev/serve.js",
    "lint": "eslint --fix --ext .js,.vue ./",
    "gen": "node ./scripts/gen.js",
    "gen:compoents": "node ./scripts/gen-compoents.js"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^13.1.0",
    "@rollup/plugin-alias": "^3.1.5",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@vue/cli-service": "^4.5.13",
    "@vue/compiler-sfc": "^3.2.6",
    "@vue/eslint-config-airbnb": "^5.3.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^7.16.0",
    "glob": "^7.1.7",
    "husky": "^6.0.0",
    "postcss": "^8.3.6",
    "postcss-prefixer": "^2.1.3",
    "rollup": "^2.52.7",
    "rollup-plugin-postcss": "^4.0.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^6.0.0",
    "sass": "^1.38.1",
    "vue": "3.2.6"
  },
  "peerDependencies": {
    "vue": "^3.2.6"
  },
  "dependencies": {
    "clamp": "^1.0.1",
    "lodash.throttle": "^4.1.1",
    "material-colors": "^1.2.6",
    "tinycolor2": "^1.4.2"
  },
  "engines": {
    "node": ">=12"
  }
}
